{
  "name": "legacy-code-modernizer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main.cjs",
  "description": "Transform Python2 Code to Python3 Code",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on http://localhost:8080 && npm run build:electron && cross-env NODE_ENV=development electron ./dist-electron/main.cjs\"",
    "build:electron": "tsc -p tsconfig.electron.json && node renameMain.cjs && cd ./backend/api_manager && cargo build --release",
    "start": "electron-forge start",
    "build:dev:linux": "vite build --mode development && cd ./backend/api_manager && cargo build --release && cd .. && pyinstaller api.py --onefile --add-binary ./api_manager/target/release/api_manager:. && mv ./dist/api .",
    "build:dev:windows": "vite build --mode development && cd ./backend/api_manager && cargo build --release && cd .. && pyinstaller api.py --onefile --add-binary ./api_manager/target/release/api_manager.exe:. && move ./dist/api.exe .",
    "lint": "eslint .",
    "preview": "vite preview",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
